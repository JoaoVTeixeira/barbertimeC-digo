<%- include("header") %>
<body>
  <main>
      <h1>Criar Conta</h1>
      <form action="/cadastrar" method="post">
          <label for="nome">
              <span>Nome</span>
              <input type="text" id="nome" name="nome">
          </label>

          <label for="telefone">
            <span>Telefone</span>
            <input type="text" id="telefone" name="telefone">
          </label>

          <label for="email">
              <span>E-mail</span>
              <input type="email" id="email" name="email">
          </label>

          <label for="senha">
              <span>Senha</span>
              <input type="password" id="senha" name="senha">
          </label>

          <input type="submit" value="Cadastrar" id="button" class="botoes">
      </form>
  </main>
  
</body>
</html>